package For;

public class for3 {
    public static void main(String[] args) {
        int contador;
       
        for(contador = 0; contador <=5000 ; contador+=2) {
               System.out.println( "Os numeros pares de 1 a 5000 sao:"+ contador );
        }
        System.out.println("Obrigado por utilizar o sistema!");
  
    }
} 
